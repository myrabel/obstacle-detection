<p id="notice"><%= notice %></p>

<h1>Smartagrics</h1>

<table>
  <thead>
    <tr>
      <th>Mositure Reads</th>
      <th>pH Reads</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @smartagrics.each do |smartagric| %>
      <tr>
        <td><%= smartagric.mositurereads %></td>
        <td><%= smartagric.phreads %></td>
        <td><%= smartagric.created_at.strftime('%B %d, %Y') %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
